{
  "name": "@digital-boss/n8n-nodes-google-pubsub",
  "version": "0.3.5",
  "description": "Google Pub/Sub Nodes for n8n.",
  "license": "MIT",
  "homepage": "https://n8n.io",
  "author": {
    "name": "Fran√ßois Voron",
    "email": "fvoron@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digital-boss/n8n-nodes-google-pubsub.git"
  },
  "scripts": {
    "dev": "npm run watch",
    "build": "npm run version:gen && tsc && gulp",
    "tslint": "tslint -p tsconfig.json -c tslint.json",
    "watch": "tsc --watch",
    "test": "jest",
    "version:gen": "genversion -s -e src/version.ts && git add . ",
    "version": "npm run build",
    "postversion": "git push"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/jest": "^27.4.0",
    "@types/node": "14.17.27",
    "@types/request-promise-native": "~1.0.15",
    "genversion": "^3.1.1",
    "gulp": "^4.0.0",
    "jest": "^27.4.7",
    "n8n-workflow": "~0.101.0",
    "ts-jest": "^27.1.3",
    "tslint": "^6.1.2",
    "typescript": "~4.6.0"
  },
  "dependencies": {
    "@google-cloud/pubsub": "^2.5.0",
    "n8n-core": "~0.119.0"
  },
  "n8n": {
    "nodes": [
      "dist/nodes/GooglePubSubTrigger.node.js"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testURL": "http://localhost/",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "/dist/",
      "/node_modules/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  }
}
